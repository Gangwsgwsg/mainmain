local placeId = game.PlaceId

if placeId == 17738975314 then
    -- Philly Street Script
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gangwsgwsg/Phillygay/refs/heads/main/Gaylo"))()
elseif placeId == 13643807539 or placeId == 14413475235 then
    -- South Bronx Script 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gangwsgwsg/Dontdeobfuscateplas/refs/heads/main/Im%20skidder%20No%20deobfuscate%20plas"))()
elseif placeId == 17306807164 then
-- no Mercy script 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gangwsgwsg/Nomercylikegay/refs/heads/main/Wsgwsg"))()
else
    
    game.StarterGui:SetCore("SendNotification", {
        Title = "Game Check";
        Text = "FALSE GAME";
        Duration = 6; -- Zeit in Sekunden, die die Benachrichtigung angezeigt wird
    })
end

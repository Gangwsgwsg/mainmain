local placeId = game.PlaceId

if placeId == 17738975314 then
    -- Philly Street Script
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gangwsgwsg/Phillygay/refs/heads/main/Gaylo"))()
elseif placeId == 13643807539 or placeId == 14413475235 then
    -- South Bronx Script 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gangwsgwsg/Southgay/refs/heads/main/Gay"))()
else
    game.StarterGui:SetCore("SendNotification", {
        Title = "Game Check";
        Text = "FALSE GAME";
        Duration = 6; -- Time in seconds the notification stays on screen
    })
end
